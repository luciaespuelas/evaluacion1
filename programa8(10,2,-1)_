def cuenta_atras() :
    for cont in range(10,-1,-1):
        print (cont)
    print ("FUERA")
cuenta_atras()

--------------------------------------------------------------------
def suma_2n() :
    suma_par=0
    for cont in range(0,200,2):
        print (cont)
        suma_par= (suma_par + cont)
    print (str("SUMA DE LOS PARES=")+ str(suma_par))
suma_2n()
